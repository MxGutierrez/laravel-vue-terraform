[
  {
    "name": "frontend",
    "image": "${IMAGE_PATH}",
    "cpu": 10,
    "memory": 100,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 3000,
        "hostPort": 3000
      }
    ],
    "environment": [
      {
        "name": "API_BASE_URL",
        "value": "${API_BASE_URL}"
      },
      {
        "name": "API_SSR_URL",
        "value": "${API_SSR_URL}"
      }
    ]
  }
]
